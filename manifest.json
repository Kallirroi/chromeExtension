{
  "manifest_version": 2,
  "description": "Turn the webpage into an attentional map.",
  "name": "clickHere",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
  	"scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "*.png"
  ]
}
